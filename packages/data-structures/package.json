  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "module": "esm/index.js",
	"files": [
    "dist/**",
    "esm/**"
  ],
  "scripts": {
		"build": "npm run build:cjs && npm run build:esm",
